@using KidKinder.Entities
@model List<Teacher>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<link rel="stylesheet" type="text/css" href="~/Templates/material_able-main/assets/css/font-awesome-n.min.css">
<div class="pcoded-inner-content">
    <!-- Main-body start -->
    <div class="main-body">
        <div class="page-wrapper">
            <!-- Page-body start -->
            <div class="page-body">
                <div class="row">
                    <div class="col-xl-4 col-md-12">
                        <div class="card mat-stat-card">
                            <div class="card-block">
                                <div class="row align-items-center b-b-default">
                                    <div class="col-sm-6 b-r-default p-b-20 p-t-20">
                                        <div class="row align-items-center text-center">
                                            <div class="col-4 p-r-0">
                                                <i class="fa fa-graduation-cap text-c-purple f-24"></i>
                                            </div>
                                            <div class="col-8 p-l-0">
                                                <h5>@ViewBag.DilEgitimCount</h5>
                                                <p class="text-muted m-b-0">Dil Öğretmeni</p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6 p-b-20 p-t-20">
                                        <div class="row align-items-center text-center">
                                            <div class="col-4 p-r-0">
                                                <img src="https://cdn-icons-png.freepik.com/512/1436/1436664.png?uid=R138619018&ga=GA1.1.1030847296.1708429824&" width="40" height="40" />
                                            </div>
                                            <div class="col-8 p-l-0">
                                                <h5>@ViewBag.Teacher</h5>
                                                <p class="text-muted m-b-0">Öğretmenlerimiz</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row align-items-center">
                                    <div class="col-sm-6 p-b-20 p-t-20 b-r-default">
                                        <div class="row align-items-center text-center">
                                            <div class="col-4 p-r-0">
                                                <img src="https://cdn-icons-png.flaticon.com/512/6577/6577251.png" width="40" height="40" />
                                            </div>
                                            <div class="col-8 p-l-0">
                                                <h5>@ViewBag.Branch</h5>
                                                <p class="text-muted m-b-0">Branşlar</p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6 p-b-20 p-t-20">
                                        <div class="row align-items-center text-center">
                                            <div class="col-4 p-r-0">
                                                <img src="data:image/png;base64," width="40" height="40" />
                                            </div>
                                            <div class="col-8 p-l-0">
                                                <h5>@ViewBag.ClassRoom</h5>
                                                <p class="text-muted m-b-0">Sınıflar</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-md-12">
                        <div class="card mat-stat-card">
                            <div class="card-block">
                                <div class="row align-items-center b-b-default">
                                    <div class="col-sm-6 b-r-default p-b-20 p-t-20">
                                        <div class="row align-items-center text-center">
                                            <div class="col-4 p-r-0">
                                                <img src="https://sm.mashable.com/mashable_tr/photo/default/turk-lirasi-tl-isareti_vdyx.jpg" width="50" height="40" />
                                            </div>
                                            <div class="col-8 p-l-0">
                                                <h5>@ViewBag.AvgPrice</h5>
                                                <p class="text-muted m-b-0">Ortalama Fiyat</p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6 p-b-20 p-t-20">
                                        <div class="row align-items-center text-center">
                                            <div class="col-4 p-r-0">
                                                <img src="https://cdn-icons-png.freepik.com/512/2720/2720641.png" width="40" height="40" />
                                            </div>
                                            <div class="col-8 p-l-0">
                                                <h5>@ViewBag.Service</h5>
                                                <p class="text-muted m-b-0">Hizmetler</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row align-items-center">
                                    <div class="col-sm-6 p-b-20 p-t-20 b-r-default">
                                        <div class="row align-items-center text-center">
                                            <div class="col-4 p-r-0">
                                                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcT96cBORRLH_jjwB0nL6H415qzni-oMfnxRSA&usqp=CAU" width="40" height="40" />
                                            </div>
                                            <div class="col-8 p-l-0">
                                                <h5>@ViewBag.Contact</h5>
                                                <p class="text-muted m-b-0">Mesajlar</p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6 p-b-20 p-t-20">
                                        <div class="row align-items-center text-center">
                                            <div class="col-4 p-r-0">
                                                <img src="https://static.thenounproject.com/png/4777262-200.png" width="40" height="40" />
                                            </div>
                                            <div class="col-8 p-l-0">
                                                <h5>@ViewBag.BookASeat</h5>
                                                <p class="text-muted m-b-0">Kayıt Sayısı</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-md-12">
                        <div class="card mat-clr-stat-card text-white green ">
                            <div class="card-block">
                                <div class="row">
                                    <div class="col-3 text-center bg-c-green">
                                        <i class="fas fa-star mat-icon f-24"></i>
                                    </div>
                                    <div class="col-9 cst-cont">
                                        <h5>@ViewBag.Testimonial</h5>
                                        <p class="m-b-0">Değerlendirmeler</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card mat-clr-stat-card text-white blue">
                            <div class="card-block">
                                <div class="row">
                                    <div class="col-3 text-center bg-c-blue">
                                        <i class="fas fa-trophy mat-icon f-24"></i>
                                    </div>
                                    <div class="col-9 cst-cont">
                                        <h5>@ViewBag.Subscribe</h5>
                                        <p class="m-b-0">Aboneler</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-6 col-md-12">
                        <div class="card table-card">
                            <div class="card-header">
                                <h5>Öğretmenlerimiz</h5>
                                <div class="card-header-right">
                                    <ul class="list-unstyled card-option">
                                        <li><i class="fa fa fa-wrench open-card-option"></i></li>
                                        <li><i class="fa fa-window-maximize full-card"></i></li>
                                        <li><i class="fa fa-minus minimize-card"></i></li>
                                        <li><i class="fa fa-refresh reload-card"></i></li>
                                        <li><i class="fa fa-trash close-card"></i></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="card-block">
                                <div class="table-responsive">
                                    <table class="table table-hover m-b-0 without-header">
                                        <tbody>
                                            @foreach (var item in Model)
                                            {
                                                <tr>
                                                    <td>
                                                        <div class="d-inline-block align-middle">
                                                            <img src="@item.ImageUrl" alt="user image" class="img-radius img-40 align-top m-r-15">
                                                            <div class="d-inline-block">
                                                                <h6>@item.NameSurname</h6>
                                                                <p class="text-muted m-b-0">@item.Branch.BranchName</p>
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-6 col-md-12">
                        <div class="row">
                            <!-- sale card start -->
                            <div class="col-md-6">
                                <div class="card text-center order-visitor-card">
                                    <div class="card-block">
                                        <h4 class="m-t-15 m-b-15"><img src="https://freepngimg.com/thumb/instagram/118541-logo-instagram-png-download-free.png" width="100" height="100"  />Instagram</h4>
                                    </div>
                                </div>
                            </div>
                           
                            <div class="col-md-6">
                                <div class="card text-center order-visitor-card">
                                    <div class="card-block">
                                        <h4 class="m-t-15 m-b-15"><img src="~/images/Twitter-X-Icon-PNG.png" width="100" height="100" style="margin-right:10px" />Twitter</h4>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="card text-center order-visitor-card">
                                    <div class="card-block">
                                        <h4 class="m-t-15 m-b-15"><img src="https://cdn1.iconfinder.com/data/icons/logotypes/32/circle-linkedin-512.png" width="100" height="100" style="margin-right:10px" />LinkedIn</h4>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="card text-center order-visitor-card">
                                    <div class="card-block">
                                       
                                        <h4 class="m-t-15 m-b-15"><img src="https://cdn-icons-png.flaticon.com/512/25/25231.png" width="100" height="100" style="margin-right:10px" />Github</h4>
                                    </div>
                                </div>
                            </div>
                            <!-- sale card end -->
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>

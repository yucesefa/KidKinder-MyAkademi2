
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<html>
<head>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
      google.charts.load("current", {packages:["corechart"]});
      google.charts.setOnLoadCallback(drawChart);
      function drawChart() {
        var data = google.visualization.arrayToDataTable([
            ['KidKinder', 'İstatistik'],
            ['Referanslar',@ViewBag.testimonial],
            ['Öğretmenler', @ViewBag.teacher],
            ['Kayıtlar',@ViewBag.bookaseat],
            ['Aboneler', @ViewBag.subscribe],
            ['Sınıflar',@ViewBag.classroom]
        ]);
        var options = {
          title: 'KidKinder İstatistik',
          is3D: true,
        };

        var chart = new google.visualization.PieChart(document.getElementById('piechart_3d'));
        chart.draw(data, options);
      }
    </script>
</head>
<body>

    <div id="piechart_3d" style="width: 900px; height: 500px; margin-top:120px; margin-left:250px;"></div>
</body>
</html>